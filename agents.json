{
  "defaults": {
    "model": {
      "primary": "zai/glm-4.7"
    },
    "models": {
      "zai/glm-4.7": {
        "alias": "GLM"
      },
      "ollama/deepcoder:14b": {
        "alias": "DeepCoder"
      },
      "openrouter/arcee-ai/trinity-large-preview:free": {
        "alias": "TrinityLarge"
      }
    },
    "workspace": "/home/john/.openclaw/workspace",
    "compaction": {
      "mode": "safeguard"
    },
    "maxConcurrent": 4,
    "subagents": {
      "maxConcurrent": 8
    },
    "sandbox": {
      "mode": "non-main",
      "workspaceAccess": "rw",
      "scope": "session",
      "docker": {
        "image": "my-openclaw-sandbox",
        "network": "host",
        "binds": [
          "/var/run/docker.sock:/var/run/docker.sock",
          "/home/john/programming:/projects:rw",
          "/home/john/.gitconfig:/root/.gitconfig:ro"
        ]
      }
    }
  },
  "list": [
    {
      "id": "value-orchestrator",
      "workspace": "/home/john/.openclaw/workspace-value-orchestrator",
      "model": "zai/glm-4.7",
      "think": "high",
      "subagents": {
        "allowAgents": [
          "algorithmic-architect",
          "cognitive-designer",
          "strategic-catalyst",
          "adaptive-operator"
        ]
      }
    },
    {
      "id": "algorithmic-architect",
      "workspace": "/home/john/.openclaw/workspace-algorithmic-architect",
      "think": "high",
      "model": "zai/glm-4.7"
    },
    {
      "id": "cognitive-designer",
      "workspace": "/home/john/.openclaw/workspace-cognitive-designer",
      "think": "high",
      "model": "zai/glm-4.7"
    },
    {
      "id": "strategic-catalyst",
      "workspace": "/home/john/.openclaw/workspace-strategic-catalyst",
      "think": "high",
      "model": "zai/glm-4.7"
    },
    {
      "id": "adaptive-operator",
      "workspace": "/home/john/.openclaw/workspace-adaptive-operator",
      "think": "high",
      "model": "zai/glm-4.7"
    },
    {
      "id": "discord-interface",
      "workspace": "/home/john/.openclaw/workspace-discord-interface",
      "model": "openrouter/arcee-ai/trinity-large-preview:free",
      "subagents": {
        "allowAgents": [
          "value-orchestrator"
        ]
      }
    }
  ]
}
